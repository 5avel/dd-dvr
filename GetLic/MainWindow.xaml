<Window x:Class="GetLic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetLic"
        mc:Ignorable="d"
        Title="MainWindow" Height="115" Width="300">
    <StackPanel Margin="5">
        <TextBox Name="tbHash" Text="Hash" />
        <TextBox Margin="0 5 0 0" Name="tbLicKey" Text="Lic" />
        <Button Margin="5" Name="btnGetLic" Content="Get Lic" Click="btnGetLic_Click"/>


    </StackPanel>
</Window>
